{
  "cache": false,
  "check-coverage": false,
  "extension": [
    ".ts"
  ],
  "include": [
    "**/*.js",
    "**/*.ts"
  ],
  "exclude": [
    "coverage/**",
    "node_modules/**",
    "**/*.d.ts",
    "**/*.test.ts",
    "dist/",
    "docs/",
    "test/",
    "example"
  ],
  "sourceMap": true,
  "reporter": [
    "html",
    "text",
    "text-summary",
    "lcov"
  ],
  "all": true,
  "instrument": true
}